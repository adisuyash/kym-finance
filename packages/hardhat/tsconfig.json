{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "types": []
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "dist", "cache", "artifacts", "typechain-types"]
}
